<Styles>
        <Style name="Headline - H2" element="h2" />
        <Style name="Headline - H3" element="h3" />
        <Style name="Headline - H4" element="h4" />
        <Style name="Headline - H5" element="h5" />
        <Style name="Headline - H6" element="h6" />
        <Style name="Paragraph" element="p" />
        <Style name="Address" element="address" />
        <Style name="Blockquote" element="blockquote" />
        <Style name="Div" element="div" />
        <Style name="Byline" element="p">
                <Attribute name="class" value="by-line" />
        </Style>
        <Style name="Photo Credit" element="p">
                <Attribute name="class" value="photo-credit" />
        </Style>
        <Style name="Pullquote" element="div">
                <Attribute name="class" value="pullquote" />
        </Style>
        <Style name="wfH1" element="span">
                <Attribute name="class" value="wfH1" />
        </Style>
</Styles>
